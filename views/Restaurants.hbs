<!DOCTYPE html>
<html>
  <head>
    <title></title>
  </head>
  <body>
    <h1>Restaurant(s) Information</h1>
    <div class="center">
      <table>
        <thead>
          <th>Name</th>
          <th>Cuisine</th>
        </thead>
        <tbody>
          {{#each restaurants}}
            <tr>
                <td>{{this.name}}</td>
                <td>{{this.cuisine}}</td>
            </tr>
          {{/each}}
        </tbody>
      </table> 
    </div>
  </body>
</html>